# Copie este arquivo para .env.local e preencha com seus valores reais

# Supabase
# IMPORTANTE: Preencha com seus valores reais do Supabase
# Você pode encontrar esses valores em: https://supabase.com/dashboard/project/_/settings/api
NEXT_PUBLIC_SUPABASE_URL=https://seu-projeto.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=sua_anon_key_aqui
SUPABASE_SERVICE_ROLE_KEY=sua_service_role_key_aqui

# AWS S3
# IMPORTANTE: Preencha com suas credenciais AWS
AWS_ACCESS_KEY_ID=sua_aws_access_key
AWS_SECRET_ACCESS_KEY=sua_aws_secret_key
AWS_REGION=us-east-1
AWS_S3_BUCKET_NAME=brasilpsd-resources
NEXT_PUBLIC_CLOUDFRONT_DOMAIN=seu-cloudfront-domain.cloudfront.net

# AWS SQS (Opcional - para processamento assíncrono de vídeos)
# Se configurado, vídeos serão processados por worker Docker em background
# Se não configurado, processamento será síncrono (fallback)
SQS_QUEUE_URL=https://sqs.us-east-2.amazonaws.com/123456789012/video-processing-queue

# Asaas (Pagamentos)
# IMPORTANTE: Configure sua API Key do Asaas
# Para sandbox: https://sandbox.asaas.com/api/v3
# Para produção: https://api.asaas.com/v3
ASAAS_API_KEY=sua_asaas_api_key_aqui
ASAAS_API_URL=https://sandbox.asaas.com/api/v3

# SMTP (Email)
# Configuração do servidor SMTP para envio de emails
# Hostinger: smtp.hostinger.com, porta 465, SSL/TLS habilitado
SMTP_HOST=smtp.hostinger.com
SMTP_PORT=465
SMTP_USER=suporte@brasilpsd.com.br
SMTP_PASSWORD=sua_senha_aqui

# App
NEXT_PUBLIC_APP_URL=http://localhost:3000
JWT_SECRET=seu_jwt_secret_aleatorio_aqui_123456789

